[ENABLE]
{$lua}
local aob=AOBScan('FF FF FF FF 03 00 00 00 37', "", 0)
local aob_count=aob.Count

local res
local fnd=false
if aob_count>1 then
	res=aob[1]
    fnd=true
elseif aob_count>0 then
       res=aob[0]
       fnd=true
end
   if fnd==true then
	local dec_res=tonumber(res,16)
	registerSymbol('base_preMatch',dec_res)
    aob.destroy()
   end

{$asm}
[DISABLE]
unregistersymbol(base_preMatch)
