{$lua}
local al = getAddressList()
local vars={}
for i=1, al.Count-1 do --skip script
    local ali=al.getMemoryRecord(i)
    local ofs=string.match(ali.Description,'base_playStyle(.%x+)%s*')
    if ofs==nil then
       ofs=string.match(ali.Description,'base_playStyle%s*')
       if ofs~=nil then
          vars['+0']=ali
       end
    else
     vars[ofs]=ali
    end
end

--vars['+0'].Value=

